<!DOCTYPE html>
<html>
<head>
    <title>TeduShop - @ViewBag.Title</title>
    <link href="~/Assets/client/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/bower_components/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/bower_components/jquery-ui/themes/redmond/jquery-ui.min.css" rel="stylesheet" />
    <!--theme-style-->
    <link href="~/Assets/client/css/style.css" rel="stylesheet" />
    <!--//theme-style-->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--fonts-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    <!--//fonts-->
    <style>
        .ui-autocomplete {
            z-index: 1000;
        }
    </style>
    <!--script-->
</head>
<body>
    <!--header-->
    @{Html.RenderAction("HeaderPartial", "Home");}

    <div class="container">

        @RenderBody()

        @*<div class="clearfix"> </div>*@
        @*</div>*@

        @RenderSection("homeProducts", required: false)


        @{ Html.RenderAction("CategoryPartial", "Home");}

    </div>
    @*</div>*@
    @*</div>*@
    <div class="clearfix"> </div>
    @{Html.RenderAction("FooterPartial", "Home");}

    <script src="~/Assets/client/js/jquery.min.js"></script>

    <script src="~/bower_components/jquery-ui/jquery-ui.min.js"></script>

    <script src="~/Assets/client/js/common.js"></script>

    @RenderSection("footerJs", required: false)

</body>
</html>